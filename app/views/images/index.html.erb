<%= link_to 'New Image', new_image_path %>
<p id="notice"><%= notice %></p>

<div class="row"> 
  <div class="column">

    <% @images[0..@quarter_count].each do |image| %>
      <img src="<%= image.image_url %>">
    <% end %>
  </div>
  <div class="column">
    <% @images[(@quarter_count + 1)..(@quarter_count * 2)].each do |image| %>
      <img src="<%= image.image_url %>">
    <% end %>
  </div> 
  <div class="column">
    <% @images[(2 * @quarter_count + 1)..(@quarter_count * 3)].each do |image| %>
      <img src="<%= image.image_url %>">
    <% end %>
  </div>
  <div class="column">
    <% @images[(3 * @quarter_count + 1)..(-1)].each do |image| %>
      <img src="<%= image.image_url %>">
    <% end %>
  </div>
</div>